proc revstack
    var tmp
    while isempty !! do
        last tmp transfer
    end
    tmp last dump
procend


proc stack_length
    var counter in counter 0 return
    var tmp_stack
    while isempty !! do
        counter last transfer 1 + last counter transfer
        last tmp_stack transfer
    end
    tmp_stack last dump revstack
    counter last dump
procend



