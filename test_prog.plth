proc N 100 procend

proc print_new_line
    "
    " print
procend

proc print_string_n_times
    dup 0 != if
        over print -1 + print_string_n_times
    else
        drop drop
    end
procend

proc main
    0
    while dup N <= do
        dup "0" over print_string_n_times
        print_new_line
        dup 1 +
    end
procend
